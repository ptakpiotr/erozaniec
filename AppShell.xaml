<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="ERozaniec.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:pages="clr-namespace:ERozaniec.Pages"
    Shell.FlyoutBehavior="Flyout"
    Title="ERozaniec">
    <ShellContent
        Title="Strona domowa"
        ContentTemplate="{DataTemplate pages:MainPage}"
        Route="Domowa" />
    <ShellContent
       Title="Różaniec"
       ContentTemplate="{DataTemplate pages:RosaryPage}"
       Route="Różaniec" />
    <ShellContent
         Title="O aplikacji"
         ContentTemplate="{DataTemplate pages:AboutPage}"
         Route="Informacje" />
</Shell>